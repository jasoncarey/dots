meh - m : yabai -m window --toggle zoom-fullscreen
meh - c : yabai -m window --grid 1:1 0:0 1:1

meh - h : yabai -m window --focus west
meh - j : yabai -m window --focus south
meh - k : yabai -m window --focus north
meh - l : yabai -m window --focus east

meh + lcmd - h : yabai -m window --swap west
meh + lcmd - j : yabai -m window --swap south
meh + lcmd - k : yabai -m window --swap north
meh + lcmd - l : yabai -m window --swap east

meh - left  : yabai -m window --resize left:-40:0
meh - right : yabai -m window --resize right:40:0
meh - up    : yabai -m window --resize top:0:-40
meh - down  : yabai -m window --resize bottom:0:40

meh - 1 : yabai -m space --focus 1
meh - 2 : yabai -m space --focus 2
meh - 3 : yabai -m space --focus 3
meh - 4 : yabai -m space --focus 4
meh - 5 : yabai -m space --focus 5

meh + lcmd - 1 : yabai -m window --space 1; yabai -m space --focus 1
meh + lcmd - 2 : yabai -m window --space 2; yabai -m space --focus 2
meh + lcmd - 3 : yabai -m window --space 3; yabai -m space --focus 3
meh + lcmd - 4 : yabai -m window --space 4; yabai -m space --focus 4
meh + lcmd - 5 : yabai -m window --space 5; yabai -m space --focus 5

meh - e : yabai -m window --display west; yabai -m display --focus west
meh - r : yabai -m window --display east; yabai -m display --focus east